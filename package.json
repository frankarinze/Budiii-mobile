{
  "name": "budiii",
  "version": "0.0.2",
  "description": "Budiii Social Interaction App",
  "author": "Naitrons Limited",
  "license": "MIT",
  "keywords": [
    "expo",
    "react native",
    "react navigation"
  ],
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "lint": "eslint ./",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest --watch --coverage=false --changedSince=origin/master",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "dependencies": {
    "@ant-design/react-native": "^3.1.9",
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "apollo-link-schema": "^1.2.3",
    "apollo-link-state": "^0.4.2",
    "apollo-utilities": "^1.3.2",
    "expo": "^33.0.5",
    "expo-asset": "^5.0.1",
    "expo-font": "~5.0.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-apollo": "^2.5.8",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-calendar-strip": "^1.3.8",
    "react-native-elements": "^1.1.0",
    "react-native-picker-select": "^6.2.0",
    "react-native-range-slider": "^0.1.2",
    "react-native-svg": "~9.4.0",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-action-buffer": "^1.2.0",
    "redux-form": "^8.2.4",
    "redux-persist": "^5.10.0",
    "redux-persist-immutable": "^4.3.1",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-thunk": "^2.3.0",
    "rn-range-slider": "^1.2.6"
  },
  "devDependencies": {
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-import-helpers": "^0.1.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js"
    ],
    "verbose": true
  },
  "private": true,
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": ">=10.3.0"
  }
}
